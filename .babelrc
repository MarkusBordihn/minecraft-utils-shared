{
  "env": {
    "commonjs": {
      "presets": [
        [
          "@babel/preset-env",
          { "targets": { "node": "current" }, "modules": "commonjs" }
        ]
      ],
      "plugins": [
        "@babel/plugin-transform-modules-commonjs",
        "babel-plugin-transform-import-meta",
        ["transform-import-extension", { "mjs": "cjs" }]
      ]
    }
  }
}
